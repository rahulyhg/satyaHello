             $("#driver_office").css("display","block");
             $("#driver_office").animate({width:'+=150px'},5000);
		$("#driver_oaddsH").val(data.path.url);
		break;
case "file_pan":
                     $("#driver_panupload_loder").hide();
                $("#driver_panupload").animate({width:'=0px'},'fast'); 
                $("#driver_panupload").css("display","block");
		$("#driver_panupload").animate({width:'+=150px'},5000);  
		$("#driver_panH").val(data.path.url);
		break;
case "file_vehicle":
                $("#driver_vehicle_loder").hide();
                $("#driver_vehicle").animate({width:'=0px'},'fast'); 
                   $("#driver_vehicle").css("display","block");
                $("#driver_vehicle").animate({width:'+=150px'},5000); 
		$("#file_vehicleH").val(data.path.url);
		break;
case "file_badge":
                    $("#driver_badge_loder").hide();
                 $("#driver_badge").animate({width:'=0px'},'fast'); 
                    $("#driver_badge").css("display","block");  
                   $("#driver_badge").animate({width:'+=150px'},5000); 
		$("#file_badgeH").val(data.path.url);
		break;
case "file_police":
                    $("#driver_police_loder").hide();
               $("#driver_police").animate({width:'=0px'},'fast'); 
             $("#driver_police").css("display","block");
      
              $("#driver_police").animate({width:'+=150px'},5000); 
		$("#file_policeH").val(data.path.url);
		break;
case "file_audit":
                    $("#driver_audit_loder").hide();
                    $("#driver_audit").animate({width:'=0px'},'fast'); 
                     $("#driver_audit").css("display","block");
                       $("#driver_audit").animate({width:'+=150px'},5000);  
		$("#file_auditH").val(data.path.url);
