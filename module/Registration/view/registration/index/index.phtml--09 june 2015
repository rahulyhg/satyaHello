<script>

   
$('.container').on('change', 'input[type="file"]' , function(e){

e.preventDefault();

   var fileName = $(this).attr("name");
    
					
		var loderForm = fileName.replace ( /[^\d.]/g, '' );
	var lodervalue = $.isNumeric(loderForm);
	 if( lodervalue == true){
			
				var name = fileName.replace(/\d+/g, '');
			switch(name){
			case "file":
                                        $("#driver_resident_loderH"+loderForm).show();
//					 $("#driver_residentH"+loderForm).animate({width:'=0px'},'fast'); 
//					$("#driver_residentH"+loderForm).css("display","block");
//					$("#driver_residentH"+loderForm).animate({width:'+=150px'},5000); 
					break;
			case "file_office":
                                        $("#driver_office_loderH"+loderForm).show();
//					$("#driver_officeH"+loderForm).animate({width:'=0px'},'fast'); 
//					$("#driver_officeH"+loderForm).css("display","block");
//					$("#driver_officeH"+loderForm).animate({width:'+=150px'},5000);
					break;
			case "file_pan":
                                        $("#driver_panupload_loderH"+loderForm).show();
//					$("#driver_panuploadH"+loderForm).animate({width:'=0px'},'fast'); 
//					$("#driver_panuploadH"+loderForm).css("display","block");
//					$("#driver_panuploadH"+loderForm).animate({width:'+=150px'},5000);
					break;
			case "file_vehicle":
                                        $("#driver_vehicle_loderH"+loderForm).show();
//					$("#driver_vehicleH"+loderForm).animate({width:'=0px'},'fast'); 
//					$("#driver_vehicleH"+loderForm).css("display","block");
//					$("#driver_vehicleH"+loderForm).animate({width:'+=150px'},5000);
					break;
			case "file_badge":
                                         $("#driver_badge_loderH"+loderForm).show();
//					$("#driver_badgeH"+loderForm).animate({width:'=0px'},'fast');
//					$("#driver_badgeH"+loderForm).css("display","block");
//					$("#driver_badgeH"+loderForm).animate({width:'+=150px'},5000);
					break;
			case "file_police":
                             $("#driver_police_loderH"+loderForm).show();
//					$("#driver_policeH"+loderForm).animate({width:'=0px'},'fast');
//					$("#driver_policeH"+loderForm).css("display","block");
//					$("#driver_policeH"+loderForm).animate({width:'+=150px'},5000);
					break;
			case "file_audit":
                            $("#driver_audit_loderH"+loderForm).show();
//					$("#driver_auditH"+loderForm).animate({width:'=0px'},'fast');
//					$("#driver_auditH"+loderForm).css("display","block");
//					$("#driver_auditH"+loderForm).animate({width:'+=150px'},5000);
				break;		
		
				}
		
		
		//alert("yes");
		
		 }
else{
	
   if(fileName == 'file'){
       $("#driver_resident_loder").show();
//       $("#driver_resident").animate({width:'=0px'},'fast'); 
//       $("#driver_resident").css("display","block");
//	    $("#driver_resident").animate({width:'+=150px'},5000); 
	   
   }
   else if(fileName == 'file_office'){
       
