function completeHandler(data){
    
    if(data.status == "success"){
 
	var appendedForm = data.path.name.replace ( /[^\d.]/g, '' );
	var avalue = $.isNumeric(appendedForm);
	 if( avalue == true){
			
				var name = data.path.name.replace(/\d+/g, '');
			switch(name){
			case "file":    
                            
                                        $("#driver_resident_loderH"+appendedForm).hide();
                                         $("#driver_residentH"+appendedForm).animate({width:'=0px'},'fast'); 
					$("#driver_residentH"+appendedForm).css("display","block");
					$("#driver_residentH"+appendedForm).animate({width:'+=150px'},5000); 
					$("#fileHa"+appendedForm).val(data.path.url);
					break;
			case "file_office": 
                                        $("#driver_office_loderH"+appendedForm).hide();
                                        $("#driver_officeH"+appendedForm).animate({width:'=0px'},'fast'); 
					$("#driver_officeH"+appendedForm).css("display","block");
					$("#driver_officeH"+appendedForm).animate({width:'+=150px'},5000);
					$("#file_officeHa"+appendedForm).val(data.path.url);
					break;
			case "file_pan":
                                        $("#driver_panupload_loderH"+appendedForm).hide();
                                        $("#driver_panuploadH"+appendedForm).animate({width:'=0px'},'fast'); 
					$("#driver_panuploadH"+appendedForm).css("display","block");
					$("#driver_panuploadH"+appendedForm).animate({width:'+=150px'},5000);
					$("#file_panHa"+appendedForm).val(data.path.url);
					break;
			case "file_vehicle":
                                        $("#driver_vehicle_loderH"+appendedForm).hide();
                                         $("#driver_vehicleH"+appendedForm).animate({width:'=0px'},'fast'); 
					$("#driver_vehicleH"+appendedForm).css("display","block");
					$("#driver_vehicleH"+appendedForm).animate({width:'+=150px'},5000);
					$("#file_vehicleHa"+appendedForm).val(data.path.url);
					break;
			case "file_badge":
                                         $("#driver_badge_loderH"+appendedForm).hide();
                                        $("#driver_badgeH"+appendedForm).animate({width:'=0px'},'fast');
					$("#driver_badgeH"+appendedForm).css("display","block");
					$("#driver_badgeH"+appendedForm).animate({width:'+=150px'},5000);
					$("#file_badgeHa"+appendedForm).val(data.path.url);
					break;
			case "file_police":
                                        $("#driver_police_loderH"+appendedForm).hide();
                                        $("#driver_policeH"+appendedForm).animate({width:'=0px'},'fast');
					$("#driver_policeH"+appendedForm).css("display","block");
					$("#driver_policeH"+appendedForm).animate({width:'+=150px'},5000);
					$("#file_policeHa"+appendedForm).val(data.path.url);
					break;
			case "file_audit":
                            $("#driver_audit_loderH"+appendedForm).hide();
                                        $("#driver_auditH"+appendedForm).animate({width:'=0px'},'fast');
					$("#driver_auditH"+appendedForm).css("display","block");
					$("#driver_auditH"+appendedForm).animate({width:'+=150px'},5000);
					$("#file_auditHa"+appendedForm).val(data.path.url);
				break;		
		
				}
		
		
		//alert("yes");
		
		 }
else{ 
    
 //alert("Uploaded file");
switch(data.path.name){
case "file":        
    
      $("#driver_resident_loder").hide();
                 $("#driver_resident").animate({width:'=0px'},'fast'); 
                $("#driver_resident").css("display","block");
                 $("#driver_resident").animate({width:'+=150px'},5000); 
		$("#driver_addsH").val(data.path.url);
