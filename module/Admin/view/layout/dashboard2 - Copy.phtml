        url: site_url,
        dataType: "json",				 
        success: function(data){
						if(data.status=='true'){
						window.location.href='<?php echo $this->basePath()."/admin";?>'; 
						}else{
						window.location.href='<?php echo $this->basePath()."/admin";?>'; 
						}
						return false;
				},
		
		error: function(error){
	alert(error);	
		}
		});
});
</script>
