function dynamicText(id){
	var url = '<?php echo $this->basePath();?>'+'/tunnel/admin/admin_text?id='+id;
	$.ajax({
		type: 'POST',
		url: url,
		dataType: "json",	 
		success: function(data){
			for(key in data.key) {
				var infoJSON = data.key[key];
				$('.'+key).html(infoJSON);
			}
		},
		error: function(error){
			console.log(error);
		}
	});
}
