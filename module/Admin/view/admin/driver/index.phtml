////// Code to Export Allocation History in CSV /////////////////


$('.export_allocation_history').click(function(event){
	//Get table
		var table = $("#driverAhistory")[0];
		//Get number of rows/columns
		var rowLength = table.rows.length;
		var colLength = table.rows[0].cells.length;
		//Declare string to fill with table data
		var tableString = "";
		var tableHeader = "";
		var tableBody = "";
		//Get column headers
		for (var j = 0; j < rowLength; j++) {
			for (var i = 0; i < colLength; i++) {
				tableString += table.rows[j].cells[i].innerHTML.split(",").join("") + ",";
			}
			tableString += "\r\n";
		}
		var date = new Date();
		date = date.toUTCString();
		//alert(date);
		//Save file
		if (navigator.appName == "Microsoft Internet Explorer") {
			//Optional: If you run into delimiter issues (where the commas are not interpreted and all data is one cell), then use this line to manually specify the delimeter
			 tableString = 'sep=,\r\n' + tableString;
			 myFrame.document.open("text/html", "replace");
			 myFrame.document.write(tableString);
			 myFrame.document.close();
			 myFrame.focus();
			 myFrame.document.execCommand('SaveAs', true, 'Allocation history data('+ date +').csv');
		 } else {
			csvData = 'data:application/csv;charset=utf-8,' + encodeURIComponent(tableString);
			 $(event.target).attr({
				 'href': csvData,
				 'target': '_blank',
				 'download': 'Allocation history('+ date +').csv'
			 });
		 }

});	

