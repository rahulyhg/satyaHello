		enquiryStatus = html.data[0].Status;
		//alert(enquiryStatus);
		var url = '<?php echo $this->basePath();?>'+'/tunnel/admin/getAllActionType';
		$.ajax({
			type: 'POST',
			url: url,
			dataType: "json",
			data:{    
					key:'',
				},
			success: function(html){
				var len = html.data.length;
				var detailComplaint = "";
				for(var i=0;i<len;i++){
					if(enquiryStatus == html.data[i][1]){
						detailComplaint += "<option value='"+html.data[i][1]+"' selected='selected'>"+html.data[i][1]+"</option>";
					}else{
						detailComplaint += "<option value='"+html.data[i][1]+"'>"+html.data[i][1]+"</option>";
					}
				}
				$("#enquiry_status").html(detailComplaint);
			},
			error: function(error){
				console.log(error);
			}
		});
	}).fail(function (error){
	alert('error');
	console.log(error);	
	});
