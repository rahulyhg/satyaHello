function getComplaintStatus(){
	data=$(this).attr("data");
	$.ajax({
		url: '<?php echo $this->basePath();?>'+"/tunnel/admin/driver_info?id="+data,
		cache: false,
		dataType:"json"
	})
	.done(function(html){
		com_data = html.data.com_id;
		$.ajax({
			url: '<?php echo $this->basePath();?>'+'/tunnel/admin/getAllComplaintById?complaint_id='+com_data,
			cache: false,
			dataType:"json"
		})
		.done(function( html ){
			complaintStatus = html.data[0].StatusID;
			//alert( "Complaint Status : " + complaintStatus );
			var url = '<?php echo $this->basePath();?>'+'/tunnel/admin/getAllActionType';
			$.ajax({
				type: 'POST',
				url: url,
				dataType: "json",
				data:{    
						key:'',
				},
				success: function(html){
					var len = html.data.length;
					var detailComplaint = "";
					for(var i=0;i<len;i++){
						if(complaintStatus == html.data[i][1]){
							detailComplaint += "<option value='"+html.data[i][1]+"' selected='selected'>"+html.data[i][1]+"</option>";
						}else{
							detailComplaint += "<option value='"+html.data[i][1]+"'>"+html.data[i][1]+"</option>";
						}
					}
					$("#complaint_status").html(detailComplaint);
					$("#complaint_status_popup").html(detailComplaint);
				},
					error: function(error){
					console.log(error);
				}
			});
		})
		.fail(function (error){
			alert('error');
			console.log(error);	
		});
	})
	.fail(function (error){
			alert('error');
			console.log(error);	
	});
}
 
