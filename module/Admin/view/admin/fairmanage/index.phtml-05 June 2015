<script>
var appendedDistanceLength = 2;
var appendedWaitingLength = 2;
var counterext = 1;
var vehicleID;
var cabTypeId;

$( document ).ready(function() {
$('#Cab-Cabtype_filter label').attr("style","display:none;");
$("#since").datepicker();
$("#since").datepicker("option","dateFormat","yy-mm-dd");
$("#to").datepicker();
$("#to").datepicker("option","dateFormat","yy-mm-dd");
	$('#nightChStrtTime').timepicker({
			hourMin: 18,
			hourMax: 24
		});	

	$('#nightChEndTime').timepicker({
			hourMin: 0,
			hourMax: 5
	});	
	
	getAllCountry();
	dynamicText(8);
	
    if (navigator.geolocation) {
        navigator.geolocation.getCurrentPosition(showPosition);
    } else { 
        alert("Geolocation is not supported by this browser.") ;
    }
	
});


function datatablsCSS(){
	$('.paging_simple_numbers').parent().addClass('pgnt_topfix');
    $('.dataTables_length').parent().removeClass('col-sm-6');
    $('.dataTables_length').parent().addClass('col-sm-2');
    $('.dataTables_length').parent().addClass('pgno_top');
	$('.pgno_top').attr("style","top:27px;");
	$('.pgnt_topfix').attr("style","top:26px;");
}

$(".addCabType").click(function(){
	$('#fairmanageId').bPopup({
		position : [200,100]
	});

});



function showPosition(position) {
  var lat = position.coords.latitude;	
  var log = position.coords.longitude;
 
  
  var url = '<?php echo $this->basePath();?>'+'/tunnel/admin/reverse_geocode';
	$.ajax({
		type: 'POST',
		url: url,
		dataType: "json",
		data : {    
				key:'',
				currentLat:lat,
				currentlong : log,
			},		 
		success: function(data){
				$("#cab-countryText").val(data.country);
				$("#cab-stateText").val(data.location);
				$("#cab-cityText").val(data.city);
				getAllCabTypes();
				getState();
				getAllCity();
			},
		error: function(error){
			console.log(error);
		}
	});
}



function getState()
{
	var defaultSt = $("#cab-stateText").val();
	
	var country = $("#cab-countryText").val();
	
	var url = '<?php echo $this->basePath();?>'+'/tunnel/admin/getAllState';
	$.ajax({
		type: 'POST',
		url: url,
		dataType: "json",
		data : {    
				key:'',
				country:country
				
			},		 
		success: function(html){
			
			var len = html.data.length;
			var detailState = "";
			for(var i=0;i<len;i++){
				var stateTx = html.data[i].state;
				if(html.data[i].state == defaultSt)	{
					detailState += "<option value='"+html.data[i].id+"' selected>"+ html.data[i].state+"</option>";
				}else{
					detailState += "<option value='"+html.data[i].id+"'>"+ html.data[i].state+"</option>";
				}
			
				}
			
				$("#addCab_stateDropdown").html(detailState);
				
				
				
		},
		error: function(error){
			console.log(error);
		}
	});

}

function getAllCountry()
{
	var cabcountryText = $("#cab-countryText").val();
	var url = '<?php echo $this->basePath();?>'+'/tunnel/admin/getAllCountry';
	$.ajax({
		type: 'POST',
		url: url,
		dataType: "json",
		data : {    
				key:'',
				
			},		 
		success: function(html){
			
			var len = html.data.length;
			var detailCountry = "";
			for(var i=0;i<len;i++){
				if(cabcountryText == html.data[i].CountryName){
					detailCountry += "<option value='"+html.data[i].ID+"' selected>"+ html.data[i].CountryName+"</option>";
				}else{
					detailCountry += "<option value='"+html.data[i].ID+"'>"+ html.data[i].CountryName+"</option>";
				}
			}
			
				$("#addCab_countryDropdown").html(detailCountry);
			
			},
		error: function(error){
			console.log(error);
		}
	});

}

function getAllCity(){
	var cabcityText = $("#cab-cityText").val();

	var stateId = $('#cab-stateText').val();
	
	var url = '<?php echo $this->basePath();?>'+'/tunnel/admin/getAllCity';
	$.ajax({
		type: 'POST',
		url: url,
		dataType: "json",
		data : {    
				key:'',
				state:stateId
				
			},		 
		success: function(html){
			
			var len = html.data.length;
			var detailCity = "";
			for(var i=0;i<len;i++){
				if(cabcityText == html.data[i].name){
					detailCity += "<option value='"+html.data[i].id+"' selecetd >"+ html.data[i].name+"</option>";
				}else{
					detailCity += "<option value='"+html.data[i].id+"'>"+ html.data[i].name+"</option>";
				}
			}
			
			$("#addCab_cityDropdown").html(detailCity);
			
			},
		error: function(error){
			console.log(error);
		}
	});



}


$("#addCab_stateDropdown").change(function() {
	var stateId = $('#addCab_stateDropdown :selected').text();
	
	var url = '<?php echo $this->basePath();?>'+'/tunnel/admin/getAllCity';
	$.ajax({
		type: 'POST',
		url: url,
		dataType: "json",
		data : {    
				key:'',
				state:stateId
			},		 
		success: function(html){
			var len = html.data.length;
			var detailCity = "";
			for(var i=0;i<len;i++){
				detailCity += "<option value='"+html.data[i].id+"'>"+ html.data[i].name+"</option>";
			
			}
				$("#addCab_cityDropdown").html(detailCity);
			},
		error: function(error){
			console.log(error);
		}
	});
});

var cabTypeTable = '';
function getAllCabTypes(){
	var city = $("#cab-cityText").val();
	var state = $("#cab-stateText").val();
	cabTypeTable = $('#Cab-Cabtype').dataTable( {
						"ajax": '<?php echo $this->basePath();?>'+'/tunnel/admin/getAllCabType?city='+city+'&state='+state,
						"deferRender": true
					} );
	datatablsCSS();
	$("#Cab-Cabtype_wrapper div:first").attr("class","tbsearchrow clearfix");
	$("#Cab-Cabtype_wrapper").attr("style","border: 1px solid #d1d1d1; margin-bottom:20px;");
	 
	$("#Cab-Cabtype_wrapper").addClass("clearfix");
		$("#Cab-Cabtype_wrapper").addClass("font12px");
	$("#Cab-Cabtype_info").parent().parent().removeClass("row");	
 
}

function searchCabTypes(){
	var city = $("#cab-cityText").val();
	var state = $("#cab-stateText").val();
	var country = $("#cab-countryText").val();
	var vehicle_no = $("#vehicleNo").val();
	var vehicle_model = $("#vehicleModel").val();
	var since = $("#since").val();
	var to = $("#to").val();
	
	var cabhtml = $(this).html();
	$("#Cab-Cabtype_wrapper").val(cabhtml);
	
	$("#Cab-Cabtype thead").html('<tr> <th>S.No.</th> <th>vehicle Number</th> <th>Cab Type</th>  <th>Vehicle Name</th> <th> Manufacturer</th> <th>Vehicle Model</th><th>Registration Date</th><th>Rate/Km</th><th>Rate/hour</th><th>Minimum Charge</th><th>Action</th></tr>');
	searchTable = $('#Cab-Cabtype').dataTable( {
						"ajax": '<?php echo $this->basePath();?>'+'/tunnel/admin/searchCabType?city='+city+'&state='+state+'&country='+country+'&vehicle_no='+vehicle_no+'&vehicle_model='+vehicle_model+'&since='+since+'&to='+to,
						"deferRender": true
					} );
	$("#Cab-Cabtype tfoot").html('<tr> <th>S.No.</th> <th>vehicle Number</th> <th>Cab Type</th>  <th>Vehicle Name</th> <th> Manufacturer</th> <th>Vehicle Model</th><th>Registration Date</th><th>Rate/Km</th><th>Rate/hour</th><th>Minimum Charge</th><th>Action</th></tr>');
	
	$("#Cab-Cabtype_wrapper").attr("style","border:1px solid #d2d2d2;");
	$("#Cab-Cabtype_wrapper").addClass("clearfix");
	$("#Cab-Cabtype_wrapper").addClass("font12px");
	$("#Cab-Cabtype_wrapper + div").removeClass("row");
	$("#Cab-Cabtype_info").parent().parent().removeClass("row");
	$("#Cab-Cabtype_wrapper div:first").attr("class","tbsearchrow clearfix");
	$('#Cab-Cabtype_filter label').attr("style","display:none;");
}

$("#searchFairmanage").click(function(){
	cabTypeTable.fnDestroy();
	searchCabTypes();
	datatablsCSS();
 
 });

$('.fairmanageFilter').keypress(function(e){
	var key=e.which;
	if(key==13)
	{
		$('#searchFairmanage').click();
	}
});

$(document).on('click','.manageVehicleFair',function(){
	
	$("#fair_manage_save").show();
	$("#fixed_route_fair_manage_vehicle").show();
	$("#premium_fair_manage_vehicle").show();
	$("#distance_fair_manage_vehicle").show();
	$("#basic_fair_manage_vehicle").show();
	$("#fixed_route_fair_manage_cab_save").hide();
	$("#premium_fair_manage_cab_save").hide();
	$("#distance_fair_manage_cab_save").hide();
	$("#basic_fair_manage_cab_save").hide();
	$("#fair_manage_cab_save").hide();
	$("#cab_package_save").hide();
	$("#vehicle_package_save").show();
	$("#vehicle_distance_hour_save").show();
	$("#cab_distance_hour_save").hide();
	$("#vehicle_distance_waiting_save").show();
	$("#cab_distance_waiting_save").hide();
	$("#package_calc_cab_save").show();
	$("#package_calc_vehicle_save").hide();
	$("#share_cab_save").show();
	
	
	var divPosition = $('#navbar-brand').offset();
      //$('html, body').animate({ scrollTop: divPosition.top }, "fast");	
	
	var id = $(this).attr('id');
	var idArr = id.split("_");
	vehicleID = idArr[1];
	
	$.ajax({
		url: '<?php echo $this->basePath();?>'+'/tunnel/admin/vehicleInfoById?vehicleId='+vehicleID,
		cache: false,
		dataType:"json"
	})
  .done(function( html ) {
	
	fairManagePopup(html);

})
.fail(function (error){
	alert('error');
	console.log(error);
		
  });

});

function fairManagePopup(html)
{
	$('#vehicleFairmanage input[type="checkbox"]').prop('checked', false);
	$('#vehicleFairmanage input[type="radio"]').prop('checked', false);
	$('#vehicleFairmanage input[type="text"]').val('');
	appendedDistanceLength = 2;	
	appendedWaitingLength = 2;
	counterext = 1;
	
	$("#append_postCodes").empty();
	
	$("#cab_vehicle_taxText").val(html.data[0].basic_tax);
	$("#cab_vehicle_rate_per_km").val(html.data[0].Per_Km_Charge);
	$("#cab_vehicle_minmum_distance").val(html.data[0].Min_Distance);
	$("#cab_vehicle_minmum_fare").val(html.data[0].MinimumCharge);
	if(html.data[0].round_up_km == 'on' ){
		$('input[name="round_up_km"]').prop('checked', true);
	}
	
	if(html.data[0].accumulated_instance == 'on' ){
		$('input[name="accum_instance"]').prop('checked', true);
	}
	
	$("#nightPremiumVehicle").val(html.data[0].NightCharges);
	if(html.data[0].NightChargesUnit == '%' ){
		$('input[name="nightCh"][value="%"]').prop('checked', true);}
	else if(html.data[0].NightChargesUnit == 'Rs' ){
		$('input[name="nightCh"][value="Rs"]').prop('checked', true);}
	
	$("#nightPremiumUnit").html(html.data[0].NightChargesUnit);
	$("#nightChStrtTime").val(html.data[0].night_rate_begins);	
	$("#nightChEndTime").val(html.data[0].night_rate_ends);
	
	$("#premiumofVehicle").val(html.data[0].premiums);
	if(html.data[0].premiumsUnit == '%' ){
		$('input[name="premiumCh"][value="%"]').prop('checked', true);}
	else if(html.data[0].premiumsUnit == 'Rs' ){
		$('input[name="premiumCh"][value="Rs"]').prop('checked', true);}
	
	$("#premiumVeUnit").html(html.data[0].premiumsUnit);
	
	var kmarr = html.data[0].rateDistanceKm;
	var rsArr = html.data[0].rateDistanceRate;
	var lengthArr = kmarr.length;
	
	$("#appended_text_form").empty();
	
	$("#mainDistanceDiv").html('<div class="row form-group"><div class="col-sm-3" style="margin-left:15px; margin-right:3px;"><input type="text" class="form-control" id="kms_1"></div><div class="col-sm-3"><div class="input-group" ><span class="input-group-addon">Rs</span><input type="text" class="form-control" id="rs_1" " ></div></div><div class="col-sm-3"  id="removeDistanceFields"><a href="#"><i class="fa fa-trash-o fa-2x"></i></a></div><div class="col-sm-3">&nbsp;</div></div><div class="row form-group"><div class="col-sm-3" style="margin-left:15px; margin-right:3px;"><input type="text" class="form-control"  id="kms_2"></div><div class="col-sm-3"><div class="input-group"><span class="input-group-addon">Rs</span><input type="text" class="form-control" id="rs_2" ></div></div><div class="col-sm-3"><div class="col-sm-4  addButton" style="padding:0;" id="add_2"><a href="#" class="btn btn-info form-control">Add</a></div></div><div class="col-sm-3">&nbsp;</div></div>');
	
	var addbtnId = $(".addButton").attr('id');
	var addbtnIdArr = addbtnId.split("_");
	var addbtnCnt = addbtnIdArr[1];
	
	if(lengthArr > addbtnCnt){
		for(var k = addbtnCnt; k < lengthArr ; k++){
			$('.addButton').trigger('click');
		}
	}
	
	for(var i=0; i< kmarr.length; i++){
		var j = i+1;
		$("#kms_"+j).val(kmarr[i]);
		$("#rs_"+j).val(rsArr[i]);
	}
	
	
	var postCodeAddId = $(".addPostCodes").attr('id');
	var postCodeAddIdArr = postCodeAddId.split("_");
	var postCodeAddCount = postCodeAddIdArr[1];
	
	var postCodePickup = html.data[0].postCodePickup;
	var postCodeDrop = html.data[0].postCodeDrop;
	var postCodeRs = html.data[0].postCodeRs;
	
	var postCodelen = postCodePickup.length;
	
	if(postCodelen > postCodeAddCount){
		for(var k = postCodeAddCount; k < postCodelen ; k++){
			$('.addPostCodes').trigger('click');
		}
	}
	
	for(var i=0; i< postCodePickup.length; i++){
		var j = i+1;
		$("#pickupPostCode_"+j).val(postCodePickup[i]);
		$("#dropoffPostCode_"+j).val(postCodeDrop[i]);
		$("#rsPostCode_"+j).val(postCodeRs[i]);
	}
	
	var extrasType=html.data[0].extraType;
	var extrasArr = html.data[0].extraRate;
	var extrasUnit = html.data[0].extraRateUnit;
	var extrLen = extrasArr.length;

	$("#append_extras").empty();
	
	$("#mainIdExtra").html('<div class="input-group addExtras" id="addExtras_1" style="margin-right:30px;"><button class="btn btn-info" style="width:90px;">Add</button></div>')
	
	var addExtraId = $(".addExtras").attr('id');
	var addExtraIdArr = addExtraId.split("_");
	var addextraCnt = addExtraIdArr[1];
	
	if(extrasArr.length > addextraCnt){
		for(var k = addextraCnt; k < extrasArr.length ; k++){
			$('.addExtras').trigger('click');
		}
	}
	for(var i = 0;i < extrasArr.length; i++ ){
		var j = i + 1;
		$("#extraIn_"+j).val(extrasArr[i]);
		$("#extraType_"+j).val(extrasType[i]);
	if(extrasUnit[i] == '%' ){
		$('input[name="extrasName_'+j+'"][value="%"]').prop('checked', true);}
	else if(extrasUnit[i] == 'Rs' ){
		$('input[name="extrasName_'+j+'"][value="Rs"]').prop('checked', true);}
	}
	
	
	$("#cab_vehicle_cancellation_charge").val(html.data[0].cancellation_fees);
	if(html.data[0].cancellation_feesUnit == '%' ){
		$('input[name="cancellationFee"][value="%"]').prop('checked', true);}
	else if(html.data[0].cancellation_feesUnit == 'Rs' ){
		$('input[name="cancellationFee"][value="Rs"]').prop('checked', true);}
	
	$("#cancellationUnit").html(html.data[0].cancellation_feesUnit);
	$("#cab_vehicle_rate_hour").val(html.data[0].tripCharge_per_hour);
	
	$("#append_waiting_fee").empty();
	$("#mainWaitningFee").html('<div class="row form-group"><div class="col-sm-3" style="margin-left:15px;"><input type="text" class="form-control" id="fair_waiting_minutes_1"></div><div class="col-sm-3" style="margin-left:3px;"><div class="input-group"><span class="input-group-addon">Rs</span><input type="text" class="form-control" id="fair_waiting_fees_1"></div></div><div class="col-sm-3" id="removeWaitingFeeFields"><a href="#"><i class="fa fa-trash-o fa-2x"></i></a></div><div class="col-sm-3">&nbsp;</div></div><div class="row form-group"><div class="col-sm-3" style="margin-left:15px;"><input type="text" class="form-control" id="fair_waiting_minutes_2"></div><div class="col-sm-3" style="margin-left:3px;"><div class="input-group"><span class="input-group-addon">Rs</span><input type="text" class="form-control" id="fair_waiting_fees_2"></div></div><div class="col-sm-3"><div class="col-sm-4 addWaitingFee" id="addWaitingFee_2" style="padding:0;"><a href="#" class="btn btn-info form-control">Add</a></div></div><div class="col-sm-3">&nbsp;</div></div>');
	
	
	var waitningfeeMinutesArr = html.data[0].waitningfeeMinutes;
	var waitingfeeValueArr = html.data[0].waitingfeeValue;
	var lengthAr = waitningfeeMinutesArr.length;
	if(lengthAr > 2){
		for(var k = 2; k < lengthAr ; k++){
			$('.addWaitingFee').trigger('click');
		}
	}
	
	for(var i=0; i< lengthAr; i++){
		var j = i+1;
		$("#fair_waiting_minutes_"+j).val(waitningfeeMinutesArr[i]);
		$("#fair_waiting_fees_"+j).val(waitingfeeValueArr[i]);
	}
	
	var rounding = html.data[0].rounding;
	
	var roundingArr = rounding.split(",");
	for(var l=0;l < roundingArr.length;l++){
		$('input[type="checkbox"][value="'+roundingArr[l]+'"]').prop('checked',true);
	}
	
	$('input[name="optionsInlineRadios1"][value="'+html.data[0].level+'"]').prop('checked', true);
	$('input[name="directionRadio"][value="'+html.data[0].direction+'"]').prop('checked', true);
	$("#first_km_rate").val(html.data[0].first_km_rate);
	
	$("#frequent_location").val(html.data[0].frequent_location);
	
	$("#rate_per_km_dw").val(html.data[0].rate_per_km_dw);
	$("#minmum_distance_dw").val(html.data[0].minimum_distance_dw);
	$("#minmum_fare_dw").val(html.data[0].minimum_fare_dw);
	
	var waiting_fees = html.data[0].waiting_fees;
	var waiting_fees_arr = waiting_fees.split("_");
	//$('input[type="text"][value="'+waiting_fees_arr[l]+'"]');
	$("#fair_waiting_minutes11").val(waiting_fees_arr[0]);
	$("#fair_waiting_fees11").val(waiting_fees_arr[1]);
	$("#fair_waiting_fees22").val(waiting_fees_arr[2]);
	
	$("#rate_per_km_dh").val(html.data[0].rate_per_km_dh);
	$("#minmum_distance_dh").val(html.data[0].minimum_distance_dh);
	$("#minmum_fare_dh").val(html.data[0].minimum_fare_dh);
	$("#rate_per_hour_dh").val(html.data[0].rate_per_hour_dh);
	
	
    $.ajax({
		url: '<?php echo $this->basePath();?>'+'/tunnel/admin/fetchCabPackage',
		cache: false,
		dataType:"json"
	})
  .done(function( html ) {
	$('#localpackage_'+html.data.localPackage).prop('checked', true);
	$('#pointToPointPackage_'+html.data.Point_To_Point_Package).prop('checked', true);
	$('#airportTransferPackage_'+html.data.Airport_Package).prop('checked', true);
	$('#outstationPackage_'+html.data.Outstation_Package).prop('checked', true);
	$("#company_share").val(html.data.company_Share);
	$("#driver_share").val(html.data.driver_Share);
	$("#partner_share").val(html.data.partner_Share);
	
  }).fail(function (error){
	alert('error');
	console.log(error);
		
 });	
	
	
	$("#vehicleFairmanage").bPopup({
			 
	}, 
        function() {
		var topd=$(document).scrollTop()+40;
          $("#vehicleFairmanage").attr("style","padding:20px;border-radius: 10px;top:"+topd+"px !important;position: absolute;z-index: 9999;opacity: 1;background:#fff;left:16% !important;right:16% !important");
        });
}



$(document).on('click','.manageCabTypeFair',function(){
	
	$("#fair_manage_save").hide();
	$("#fixed_route_fair_manage_vehicle").hide();
	$("#premium_fair_manage_vehicle").hide();
	$("#distance_fair_manage_vehicle").hide();
	$("#basic_fair_manage_vehicle").hide();
	$("#vehicle_package_save").hide();
	$("#fixed_route_fair_manage_cab_save").show();
	$("#premium_fair_manage_cab_save").show();
	$("#distance_fair_manage_cab_save").show();
	$("#basic_fair_manage_cab_save").show();
	$("#basic_fair_manage_cab_save button").show();
	$("#cab_distance_waiting_save").show();
	$("#vehicle_distance_waiting_save").hide();
	$("#vehicle_distance_hour_save").hide();
	$("#DistanceWaiting").show();
	$("#cab_distance_hour_save").show();
	
	$("#fixed_route_fair_manage_cab_save button").show();
	$("#premium_fair_manage_cab_save button").show();
	$("#distance_fair_manage_cab_save button").show();
	$("#fair_manage_cab_save").show();
	$("#package_calc_cab_save").show();
	$("#package_calc_vehicle_save").hide();
	$("#share_cab_save").show();
	
	
	
	var divPosition = $('#navbar-brand').offset();
      //$('html, body').animate({ scrollTop: divPosition.top }, "fast");	
	
	var id = $(this).attr('id');
	var idArr = id.split("_");
	cabTypeId = idArr[1];
	
	$.ajax({
		url: '<?php echo $this->basePath();?>'+'/tunnel/admin/cabTypeInfoById?cabType='+cabTypeId,
		cache: false,
		dataType:"json"
	})
  .done(function( html ) {
	
	fairManagePopup(html);
	

}).fail(function (error){
	alert('error');
	console.log(error);
		
  });


});

$('#Cab-Cabtype').on('click','.clickCabType',function(){
		var city = $("#cab-cityText").val();
		var cabId = $(this).attr('data');
		
		var cabhtml = $(this).html();
		
		$("#cab-vehicleCabTypText").val(cabhtml);
		//clearInterval(myVar2); 
        //clearInterval(myVar);
      
      if($(this).hasClass('open'))
      {
		 // myVar2=setInterval(function(){myTimer2()}, 5000);	  
          $(".remove div:first").slideUp(400,function(){
               $('.remove').remove();
          });
      $(this).removeClass('open');
	}
	else
	{
      $('.remove').remove();
       $(this).addClass('open');
      $(this).parent().parent().after('<tr class="remove" ><td colspan="10"><div style=""> <div class=""> <div class="portlet portlet-default"> <div class="portlet-heading"> <div class="portlet-title"><h5 class="h4text" style="background: #E2E2E2;padding:7px;margin: 0;">Vehicle Information</h5></div> <div class="clearfix"></div> </div> <div class="portlet-body"> <ul id="myTab" class="nav nav-tabs tabsdesign"> <li class="active"><a href="#cab_vehicle" data-toggle="tab"><i class="fa fa-list" style="color:#; margin:5px"></i>Status</a> </li> </ul> <div id="myTabContent" class="tab-content contantboxoftabs"> <div class="tab-pane fade in active" id="cab_vehicle"> <table id="exampleVehicle" class="table table-striped table-responsive margin10"><thead> <tr> <th>S.No.</th> <th>vehicle Number</th> <th>Cab Type</th>  <th>Vehicle Name</th> <th> Manufacturer</th> <th>Vehicle Model</th><th>Status</th><th>Rate/Km</th><th>Rate/hour</th><th>Minimum Charge</th><th style="width:110px !important;"></th> </tr> </thead> <tbody> </tbody> </table> </div></div></td></tr>');
	  
    data=$(this).attr("data");
    $.ajax({
		url: '<?php echo $this->basePath();?>'+'/tunnel/admin/vehicleInfo?cabId='+data+'&city='+city,
		cache: false,
		dataType:"json"
	})
  .done(function( html ) {
	var k=1;
	var statusVehicle;
    for(var i=0;i<html.data.length;i++)
	{
		statusVehicle = (html.data[i].status == 0) ?'Inactive':'Active';
		$('#exampleVehicle tbody').append('<tr><td>'+k++ +'</td><td>'+html.data[i].vehicleNumber+'</td><td>'+html.data[i].CabType+'</td><td>'+html.data[i].name+'</td><td>'+html.data[i].Manufacturer+'</td><td>'+html.data[i].VehicleModel+'</td><td>'+statusVehicle+'</td><td>'+html.data[i].Per_Km_Charge+'</td><td>'+html.data[i].tripCharge_per_minute+'</td><td>'+html.data[i].MinimumCharge+'</td><td><button id="vehicle_'+html.data[i].id+'" class="btn btn-primary manageVehicleFair">Manage Fair</button></td></tr>');  
	}
  }).fail(function (error){
	alert('error');
	console.log(error);
		
 });	
		
}		
});
 
$(document).on('click','.addButton',function(){ 
		var addButtonId = $(this).attr('id');
		var counterArr = addButtonId.split("_");
		
		var counter = counterArr[1];
			counter++;
		$("#"+addButtonId).remove();	
		
		var detailsdiv = "";
		detailsdiv += '<div class="row form-group"><div class="col-sm-3"><input type="text" class="form-control" id="kms_'+counter+'" ></div><div class="col-sm-3" ><div class="input-group"><span class="input-group-addon">Rs</span><input type="text" class="form-control" id="rs_'+counter+'"  ></div></div><div class="col-sm-3"><div class="col-sm-4  addButton" style="padding:0;" id="add_'+counter+'"><a href="#" class="btn btn-info form-control">Add</a></div></div><div class="col-sm-3">&nbsp;</div></div>';
		appendedDistanceLength ++;
		$("#appended_text_form").append(detailsdiv);
		return false;
 
 });
 

$(document).on('click','#removeDistanceFields',function(){
	var addId = $(".addButton").attr('id');
	var addIdArr = addId.split("_");
	var counter  = addIdArr[1];
	counter--;
	$(".addButton").closest('.row').remove();
	$("#rs_"+counter).closest('.col-sm-3').after('<div class="col-sm-3"><div class="col-sm-4  addButton" style="padding:0;" id="add_'+counter+'"><a href="#" class="btn btn-info form-control">Add</a></div></div>');
	appendedDistanceLength --;
	return false;
});

$(document).on('click','#removeExtraFields',function(){
	var addId = $(".addExtras").attr('id');
	var addIdArr = addId.split("_");
	var counte  = addIdArr[1];
	counte--;
	$(".addExtras").closest('.rowextra').remove();
	
	
	$("input[name='extrasName_"+counte+"'][value='Rs']").closest('.input-group').after('<div class="input-group addExtras" id="addExtras_'+counte+'" style="margin-right:30px;"><button class="btn btn-info" style="width:90px;">Add</button></div>');
	counterext --;
	return false;

});

$(document).on('click','.addExtras',function(){
	
	$("#addExtras_"+counterext).remove();
	
	counterext++;
	$("#append_extras").append('<div style="margin-top:10px;" class="rowextra"><div class="input-group" style="margin-right:30px;"><input type="text" id= "extraType_'+counterext+'" class="form-control" placeholder="Type"></div><div class="input-group" style="margin-right:30px;"><input type="text" id= "extraIn_'+counterext+'" class="form-control" placeholder="Amount"></div><div class="input-group"><input type="radio" value="%" name="extrasName_'+counterext+'" class="form-control inputsize"><label class="checkbox-inline">%</label></div><div class="input-group"><input type="radio" value="Rs" name="extrasName_'+counterext+'" class="form-control inputsize"><label class="checkbox-inline">Rs</label></div><div class="input-group addExtras" id="addExtras_'+counterext+'" style="margin-right:30px;"><button class="btn btn-info" style="width:90px;">Add</button></div></div>');
	
	return false;

});

