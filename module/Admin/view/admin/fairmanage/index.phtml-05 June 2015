var counterPostCode = 1;
$(document).on('click','.addPostCodes',function(){
	counterPostCode++;
	
	var appended = '<div class="form-group clearfix"><div class="col-sm-3" style="margin-right:3px;"><input type="text" class="form-control" id="pickupPostCode_'+counterPostCode+'"></div><div class="col-sm-3" style="margin-right:3px;"><input type="text" class="form-control" id="dropoffPostCode_'+counterPostCode+'"></div><div class="col-sm-3"><div class="input-group"><span class="input-group-addon">Rs</span><input type="text" class="form-control" id="rsPostCode_'+counterPostCode+'"></div></div></div>';
	$(this).attr("id","'addPostCodes_"+counterPostCode+"'");
	$("#append_postCodes").append(appended);
});

</script>
