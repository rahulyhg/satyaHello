$("#fixed_route_fair_manage_cab_save").click(function(){

	var frequent_location = $("#frequent_location").val();
	var pickupPostCodeArr = "";
	var dropPostCodeArr = "";
	var rsPostCodeArr = "";
	
	for(var i=1; i<= counterPostCode;i++){
		var pickupPostCode = $("#pickupPostCode_"+i).val();
		pickupPostCodeArr += pickupPostCode+"_";
		
		var dropPostCode = $("#dropoffPostCode_"+i).val();
		dropPostCodeArr += dropPostCode+"_";
		
		var rsPostCode = $("#rsPostCode_"+i).val();
		rsPostCodeArr += rsPostCode+"_";
		
	}
		
	pickupPostCodeArr = pickupPostCodeArr.slice(0, -1);
	dropPostCodeArr = dropPostCodeArr.slice(0, -1);
	rsPostCodeArr = rsPostCodeArr.slice(0, -1);
	
	$.ajax({
		url: '<?php echo $this->basePath();?>'+'/tunnel/admin/saveFairManageFixedRoute',
		cache: false,
		dataType:"json",
		data:{
				frequent_location:frequent_location,
				pickupPostCodeArr:pickupPostCodeArr,
				dropPostCodeArr:dropPostCodeArr,
				rsPostCodeArr:rsPostCodeArr,
				CabTypeId:cabTypeId
		}
		
	})
  .done(function( html ) {
		alert("Fair Updated");
		location.reload();
		//console.log(html);
	
  }).fail(function (error){
		alert(error);
		console.log(error);
		
 });

});



$("#fair_manage_cab_save").click(function(){
	var cab_vehicle_taxText = $("#cab_vehicle_taxText").val();
	var chkId = '';
	  $('.chkNumber:checked').each(function () {
		chkId += $(this).val() + ",";
	});
	chkId = chkId.slice(0, -1);
	  
	var rounding  = chkId;
	
	var leveli = $('input[name="optionsInlineRadios1"]:checked').val();
	var Direction = $('input[name="directionRadio"]:checked').val();
	
	var rate_per_km = $("#cab_vehicle_rate_per_km").val();
	var first_km_rate = $("#first_km_rate").val();
	
	var vehicle_minmum_distance = $("#cab_vehicle_minmum_distance").val();
	var vehicle_minmum_fare = $("#cab_vehicle_minmum_fare").val(); 
	
		var cancelFeeUnit = $("input[name='cancellationFee']:checked").val();
		var cnclFee = $("#cab_vehicle_cancellation_charge").val();
	
	var CancelFee = cnclFee+" "+cancelFeeUnit;
	
		var kmUpTo = "";
		var ratePerKm = "";
		for(var i=1; i<= appendedDistanceLength;i++){
			var kmi = $("#kms_"+i).val();
			kmUpTo += kmi+"_";
			var rsi = $("#rs_"+i).val();
			ratePerKm += rsi+"_";
		}
	
	kmUpTo = kmUpTo.slice(0, -1);
	ratePerKm = ratePerKm.slice(0, -1);
	
		var nightPremiumUnit = $("input[name='nightCh']:checked").val();
		var nightPremium = $("#nightPremiumVehicle").val(); 
	
	var nightChrge = nightPremium+" "+nightPremiumUnit;
	var nightChStrtTime = $("#nightChStrtTime").val();
	var nightChEndTime = $("#nightChEndTime").val();	
	
		var extrasIn = "";
		var extrasUnit = "";
		for(var i=1; i<= counterext;i++){
			var extrasUpTo = $("#extraIn_"+i).val();
			extrasIn += extrasUpTo+"_";
			var extrasUnitUpTo = $('input[name=extrasName_'+i+']:checked').val();
			extrasUnit += extrasUnitUpTo+"_";
		}
	extrasIn = extrasIn.slice(0, -1);
	extrasUnit = extrasUnit.slice(0, -1);
	var vehicle_rate_hour = $("#cab_vehicle_rate_hour").val();

		var minuteUpTo = "";
		var fee_per_minutes = "";
		for(var i=1; i<= appendedWaitingLength;i++){
			var waitingMinutei = $("#fair_waiting_minutes_"+i).val();
			minuteUpTo += waitingMinutei+"_";
			var waitnigFeei = $("#fair_waiting_fees_"+i).val();
			fee_per_minutes += waitnigFeei+"_";
		}
	
	minuteUpTo = minuteUpTo.slice(0, -1);
	fee_per_minutes = fee_per_minutes.slice(0, -1);
	
	var premiumofVehicle = $("#premiumofVehicle").val();
	var premiumofVehicleUnit = $("input[name='premiumCh']:checked").val();
	
	var premiumo = premiumofVehicle+" "+premiumofVehicleUnit;
	var frequent_location = $("#frequent_location").val();
	
	
		var pickupPostCodeArr = "";
		var dropPostCodeArr = "";
		var rsPostCodeArr = "";
		
		
		for(var i=1; i<= counterPostCode;i++){
			var pickupPostCode = $("#pickupPostCode_"+i).val();
			pickupPostCodeArr += pickupPostCode+"_";
			
			var dropPostCode = $("#dropoffPostCode_"+i).val();
			dropPostCodeArr += dropPostCode+"_";
			
			var rsPostCode = $("#rsPostCode_"+i).val();
			rsPostCodeArr += rsPostCode+"_";
			
		}
		
	pickupPostCodeArr = pickupPostCodeArr.slice(0, -1);
	dropPostCodeArr = dropPostCodeArr.slice(0, -1);
	rsPostCodeArr = rsPostCodeArr.slice(0, -1);
	
	
	$.ajax({
		url: '<?php echo $this->basePath();?>'+'/tunnel/admin/saveFairManagementOfCab',
		cache: false,
		dataType:"json",
		data:{cab_vehicle_taxText:cab_vehicle_taxText,rounding:rounding,leveli:leveli,Direction:Direction,rate_per_km:rate_per_km,first_km_rate:first_km_rate,vehicle_minmum_distance:vehicle_minmum_distance,vehicle_minmum_fare:vehicle_minmum_fare,CancelFee:CancelFee,kmUpTo:kmUpTo,ratePerKm:ratePerKm,nightChrge:nightChrge,nightChStrtTime:nightChStrtTime,nightChEndTime:nightChEndTime,extrasIn:extrasIn,extrasUnit:extrasUnit,vehicle_rate_hour:vehicle_rate_hour,minuteUpTo:minuteUpTo,fee_per_minutes:fee_per_minutes,premium:premiumo,CabTypeId:cabTypeId,frequent_location:frequent_location,pickupPostCodeArr:pickupPostCodeArr,dropPostCodeArr:dropPostCodeArr,rsPostCodeArr:rsPostCodeArr}
		
	})
  .done(function( html ) {
		alert("Fair Updated");
		location.reload();
		//console.log(html);
	
  }).fail(function (error){
		alert(error);
		console.log(error);
		
 });


});



