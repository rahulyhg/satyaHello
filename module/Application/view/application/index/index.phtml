function callLocalFare(bookingType,localCabIn,localCabFor,pickuparea,localAddress,pickuplocation,time,localNoCars) {
	var site_url = jQuery.cookie('BaseUrl');	
	var url = site_url+'/tunnel/bookingCabFare/localBookingFare';
	$.ajax({
		type: 'POST',
		url: url,
		dataType: "json",
		data : {
			bookingType:bookingType,
			localCabInData : localCabIn,
			localCabForData:localCabFor,
			pickup :pickuparea, 
			localAddressData:localAddress,
			picklatlong:pickuplocation,
			picktime:time,
			key:API_KEY,
			},		 
			success: function(data){	
			if(data.status=="true")
					{
						var totalEconomyPrice	=	localNoCars*data.totalbillEconomy;
						var totalSedanPrice		=	localNoCars*data.totalbillSedan;
						var totalPrimePrice		=	localNoCars*data.totalbillPrime;
						$('.economy').attr('title',"Minimum Charge: "+totalEconomyPrice);
						$('.sedan').attr('title'," Minimum Charge: "+totalSedanPrice);
						$('.prime').attr('title',"Minimum Charge: "+totalPrimePrice);
					}
		},
		error: function(error){
			console.log(error);			
		}
	});	
}

